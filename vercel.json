{
    "name": "masonjar-stats",
    "version": 2,
    "builds": [
        {
            "src": "build/public/**",
            "use": "@vercel/static"
        },
        {
            "src": "build/server.js",
            "use": "@vercel/node"
        }
    ],
    "build": {
      "cmd": "npm run build:prod"
    },
    "start": {
      "cmd": "npm run start:prod",
      "env": {
        "NODE_ENV": "production",
      }
    },
    "routes": [
        { "src": "/favicon.ico", "dest": "build/public/favicon.ico" },
        { "src": "/robots.txt", "dest": "build/public/robots.txt" },
        { "src": "/static/(.*)", "dest": "build/public/static/$1" },
        { "src": "/(.*)", "dest": "build/server.js" }
    ]
}